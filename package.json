{
  "name": "@swapnachalla/document-viewer",
  "version": "1.0.4",
  "description": "A React component library for viewing and comparing multiple documents side by side",
  "keywords": [
    "react",
    "pdf",
    "document",
    "viewer",
    "compare",
    "multi-document",
    "zoom",
    "download",
    "responsive",
    "typescript"
  ],
  "author": "Swapna Challa <swapna@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swapnachalla/document-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/swapnachalla/document-viewer/issues"
  },
  "homepage": "https://github.com/swapnachalla/document-viewer#readme",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:lib",
    "build:types": "tsc --project tsconfig.declarations.json",
    "build:lib": "mkdir -p dist && npm run build:types && rollup -c rollup.config.mjs",
    "build:demo": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build:lib",
    "prepare": "npm run build:lib"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^16.3.0",
    "rollup": "^3.29.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
